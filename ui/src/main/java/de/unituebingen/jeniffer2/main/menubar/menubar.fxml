<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>

<MenuBar styleClass="menu-bar" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
         fx:controller="de.unituebingen.jeniffer2.main.menubar.MenuBarPresenter">
    <menus>
        <Menu mnemonicParsing="false" text="%file">
            <items>
                <MenuItem mnemonicParsing="false" text="%open" fx:id="openMenuItem" onAction="#onOpen"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem mnemonicParsing="false" text="%saveAs" fx:id="saveAsMenuItem" onAction="#onSaveAs"/>
            </items>
        </Menu>
    </menus>
</MenuBar>
